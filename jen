node {
    stage('git checkout') { 
        git branch: 'main', url: 'https://github.com/khadar155/terraform_pratice.git'
        
    }
    stage('init') {
        dir('sirss3'){
        sh 'terraform init'
        }
        
    }
    
    stage('apply') {
        dir('sirss3'){
         sh 'terraform apply -auto-approve'
        }
        
    }
}
